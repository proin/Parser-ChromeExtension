{
    "manifest_version": 2,
    "name": "Parser",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "parser.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage"
    ]
}